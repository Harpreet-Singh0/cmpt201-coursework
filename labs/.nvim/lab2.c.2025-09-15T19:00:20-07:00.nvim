#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
int main() {
  char *buffptr = NULL;
  size_t buffsize = 0;
  char prog[20] = "/usr/bin/zsh";
  pid_t pid;
  while (1) {
    printf("Enter programs to run.\n");
    getline(&buffptr, &buffsize, stdin);

    pid = fork();
    if (pid == 0) {

      execl(prog, buffptr, (char *)NULL);
    }
  }
  return 0;
}
